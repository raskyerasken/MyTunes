   <children>
      <Label layoutX="98.0" layoutY="14.0" text="New/Edit Song" />
      <Label layoutX="35.0" layoutY="40.0" text="Category" />
      <Button fx:id="chooseDirectory" layoutX="35.0" layoutY="82.0" mnemonicParsing="false" onAction="#chooseDirectory" text="Choose.." />

      <Button fx:id="closeButton" layoutX="231.0" layoutY="82.0" mnemonicParsing="false" onAction="#closeSongview" text="Cancel" />
      <TextField fx:id="txtCategory" layoutX="98.0" layoutY="36.0" />
   </children>
</AnchorPane>
